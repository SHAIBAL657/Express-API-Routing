// Map Prsentattion
#guestModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='exampleModalLabel' aria-hidden='true')
  .modal-dialog(role='document')
    .modal-content
      .modal-header
        h5.modal-title Enter Guest Number
        button.close(type='button' data-dismiss='modal' aria-label='Close')
          span(aria-hidden='true') &times;
      .modal-body
        .guest-radio
          button#g-m.g-dot.m-2 -
          span#g-c.g-dot.g-count.m-2 0
          button#g-p.g-dot.m-2 +
      .modal-footer
        button.btn.darkorange(type='button' data-dismiss='modal') Apply
#priceModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='exampleModalLabel' aria-hidden='true')
  .modal-dialog(role='document')
    .modal-content
      .modal-header
        h5.modal-title Price Range: &#x9F3;0 - &#x9F3;30000+ per night
        button.close(type='button' data-dismiss='modal' aria-label='Close')
          span(aria-hidden='true') &times;
      .modal-body
        .price-range-slider
          p.range-value
            input#amount(type='text' readonly='')
          #slider-range.range-bar
      .modal-footer
        button.btn.darkorange(type='button' data-dismiss='modal') Apply
#searchModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='exampleModalLabel' aria-hidden='true')
  .modal-dialog(role='document')
    .modal-content
      .modal-header
        h5.modal-title Confirm given Details
        button.close(type='button' data-dismiss='modal' aria-label='Close')
          span(aria-hidden='true') &times;
      .modal-body
        p#popup.search-radio
      .modal-footer
        button.btn.darkorange(type='button' data-dismiss='modal') Apply
