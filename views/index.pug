// Navbar
#navall
  nav.navbar.navbar-light.bg-light
    #drop.dropdown
      button#dropdownMenuButton1.btn(type='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
        i.fa.fa-bars.bar
      #menu.dropdown-menu(aria-labelledby='dropdownMenuButton')
        td(scope='row')
          a.dropdown-item(href='#') Home
        td(scope='row')
          a.dropdown-item(href='#') Bookmarked
        td(scope='row')
          a.dropdown-item(href='#') List Your Property
        td(scope='row')
          a.dropdown-item(href='#') About
        td(scope='row')
          a.dropdown-item(href='#') FAQ
        td(scope='row')
          a.dropdown-item(href='#') Blog
    .col
      a.navbar-brand(href='https://www.bedroomvillas.com/')
        img(src='../../image/bedroomvillas-logo-new.png' alt='')
    form.form-inline(autocomplete='off')
      input#place.form-control.col.g-2.m-1(type='text' placeholder='Enter Your Destination')
      input#checkin.form-control.col.g-2.m-1(type='text' placeholder='Check In')
      input#checkout.form-control.col.g-2.m-1(type='text' placeholder='Check Out')
      button.btn.btn-dark.m-1(type='button' data-toggle='modal' data-target='#guestModal') Guest
      button.btn.btn-dark.m-1(type='button' data-toggle='modal' data-target='#priceModal') Price
      button#search.btn.btn-dark.m-1(type='button' data-toggle='modal' data-target='#searchModal') Search
  nav.navbar-2.navbar-light.bg-light.py-2.justify-content-between
    .second-nav-left.col.ml-2
      a(href='#')
        b Arizona
      a(href='#') Search Over 15 Properties
    .second-nav-right.col
      span.float-right
        | Sort By &nbsp;&nbsp; 
        button#dropdownMenuButton2.btn.btn-dark.dropdown-toggle(type='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false') Most Popular
        #menu.dropdown-menu(aria-labelledby='dropdownMenuButton')
          a.dropdown-item(href='#') Most Popular
          a.dropdown-item(href='#') Price:High to Low
          a.dropdown-item(href='#') Price:Low to High
          a.dropdown-item(href='#') Rating:High to Low
          a.dropdown-item(href='#') Rating:Low to High
